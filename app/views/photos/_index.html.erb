<p id="notice"><%= notice %></p>

<h1 id='photo'>Photos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @photos.each do |photo| %>

      <tr>
        <td><div style="display: none;" id ='<%=photo.title%>'><%=image_tag photo.userimage.url(:medium) %>
          <%=photo.title%>
          <%= link_to 'Show', photo %></div></td>

      </tr>

    <% end %>
  </tbody>
</table>
